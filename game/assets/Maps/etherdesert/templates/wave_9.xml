<template>
    <entity>
        <name>Flying Minion</name>
        <isMinion/>
        <!--model-->
        <model>Models/flying_minion/skin_default.xml</model>
        <walkAnimation>
            <entity>
                <name>walk</name>
            </entity>
        </walkAnimation>
        <autoAttackAnimation>
            <entity>
                <name>auto_attack</name>
            </entity>
        </autoAttackAnimation>
        <!--physics-->
        <hitbox>
            <circle radius="1"/>
        </hitbox>
        <intersectionPush/>
        <collisionGroup targetOf="map|units|spell_targets" targets="units"/>
        <hitboxActive/>
        <!--stats-->
        <isAlive/>
        <baseAttributes>
            <entity>
                <bonusFlatMaximumHealth>1800</bonusFlatMaximumHealth>
                <bonusFlatAttackDamage>100</bonusFlatAttackDamage>
                <bonusFlatAttackSpeed>1.3</bonusFlatAttackSpeed>
                <bonusFlatArmor>80</bonusFlatArmor>
                <bonusFlatMagicResistance>80</bonusFlatMagicResistance>
                <bonusFlatWalkSpeed>5</bonusFlatWalkSpeed>
            </entity>
        </baseAttributes>
        <requestUpdateAttributes/>
        <sightRange>28</sightRange>
        <isTargetable/>
        <isVulnerable/>
        <bounty>
            <entity>
                <bountyCreepScore></bountyCreepScore>
                <bountyGold>40</bountyGold>
            </entity>
        </bounty>
        <!--spells-->
        <autoAttack>
            <entity template="spells/ranged_autoattack"/>
        </autoAttack>
        <autoAggro>20</autoAggro>
    </entity>
</template>