                        <panel childLayout="horizontal">
                            <panel width="10px"/>
                            <panel childLayout="vertical">
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/health_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_health" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/walk_speed_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_walk_speed" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/attack_damage_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_attack_damage" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/attack_speed_16.png"/>
