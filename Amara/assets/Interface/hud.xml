                                    <text id="attribute_value_attack_speed" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/armor_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_armor" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                            </panel>
                            <panel width="10px"/>
                            <panel childLayout="vertical">
                                <panel height="40px"/>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/ability_power_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_ability_power" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/cooldown_speed_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_cooldown_speed" text="150" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                                <panel childLayout="horizontal">
                                    <image width="16px" height="16px" filename="Interface/hud/attributes/magic_resistance_16.png"/>
                                    <panel width="5px"/>
                                    <text id="attribute_value_magic_resistance" text="???" textHAlign="left" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                            </panel>
                        </panel>
                    </panel>
                    <panel/>
                </panel>
                <panel height="7px"/>
            </panel>
        </layer>
        <layer id="inventory" childLayout="center">
            <panel childLayout="vertical">
                <panel/>
                <panel childLayout="horizontal" height="130px">
                    <panel width="174px"/>
                    <panel childLayout="vertical" width="132px">
                        <panel childLayout="horizontal" height="42px">
                            <panel childLayout="center" width="34%" height="100%">
                                <image id="inventory_item_0_image" filename="Interface/hud/items/none.png" width="100%" height="100%"></image>
                                <panel id="inventory_item_0_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="inventory_item_0_cooldown_time" text="???" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                            </panel>
                            <panel childLayout="center" width="34%" height="100%">
                                <image id="inventory_item_1_image" filename="Interface/hud/items/none.png" width="100%" height="100%"></image>
                                <panel id="inventory_item_1_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="inventory_item_1_cooldown_time" text="???" font="Interface/fonts/Verdana_12.fnt"/>
