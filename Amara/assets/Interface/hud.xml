                        <panel childLayout="horizontal" height="42px">
                            <panel childLayout="center" width="34%" height="100%">
                                <image id="inventory_item_3_image" filename="Interface/hud/items/none.png" width="100%" height="100%"></image>
                                <panel id="inventory_item_3_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="inventory_item_3_cooldown_time" text="???" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                            </panel>
                            <panel childLayout="center" width="34%" height="100%">
                                <image id="inventory_item_4_image" filename="Interface/hud/items/none.png" width="100%" height="100%"></image>
                                <panel id="inventory_item_4_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="inventory_item_4_cooldown_time" text="???" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                            </panel>
                            <panel childLayout="center" width="34%" height="100%">
                                <image id="inventory_item_5_image" filename="Interface/hud/items/none.png" width="100%" height="100%"></image>
                                <panel id="inventory_item_5_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="inventory_item_5_cooldown_time" text="???" font="Interface/fonts/Verdana_12.fnt"/>
                                </panel>
                            </panel>
                        </panel>
                        <panel childLayout="horizontal" backgroundColor="#000D">
                            <panel width="32px"/>
                            <panel childLayout="vertical" width="26px">
                                <panel height="7px"/>
                                <panel childLayout="horizontal">
                                    <image filename="Interface/hud/gold.png" width="26px" height="26px"></image>
                                </panel>
                            </panel>
                            <panel width="7px"/>
                            <panel childLayout="vertical">
                                <panel height="11px"/>
                                <text id="gold" text="????" textHAlign="left" font="Interface/fonts/Verdana_14.fnt"/>
                            </panel>
                            <interact onClick="toggleShopVisible()"/>
                        </panel>
                        <panel height="6px"/>
                    </panel>
                    <panel/>
                </panel>
            </panel>
        </layer>
        <layer childLayout="center">
            <panel childLayout="vertical">
                <panel/>
                <panel childLayout="horizontal" height="200px">
                    <image filename="Interface/hud/unit.png" width="400px" height="100%"></image>
                    <panel/>
                </panel>
            </panel>
        </layer>
        <layer childLayout="center">
            <panel childLayout="vertical">
                <panel/>
                <panel childLayout="horizontal" height="160px">
                    <panel width="*"/>
                    <panel childLayout="vertical" width="575px">
                        <panel height="35px"/>
                        <panel childLayout="horizontal" height="58px">
                            <panel width="113px"/>
                            <panel childLayout="vertical" width="46px">
                                <panel height="5px"/>
                                <image id="passive_image" filename="Interface/hud/spells/none.png" width="100%" height="100%w"></image>
                            </panel>
                            <panel width="11px"/>
                            <panel childLayout="center" width="58px" height="100%w">
                                <image id="spell_0_image" filename="Interface/hud/spells/none.png" width="100%" height="100%"></image>
                                <panel id="spell_0_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="spell_0_cooldown_time" text="???" font="Interface/fonts/Verdana_18.fnt"/>
                                </panel>
                            </panel>
                            <panel width="2px"/>
                            <panel childLayout="center" width="58px" height="100%w">
                                <image id="spell_1_image" filename="Interface/hud/spells/none.png" width="100%" height="100%"></image>
                                <panel id="spell_1_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="spell_1_cooldown_time" text="???" font="Interface/fonts/Verdana_18.fnt"/>
                                </panel>
                            </panel>
                            <panel width="2px"/>
                            <panel childLayout="center" width="58px" height="100%w">
                                <image id="spell_2_image" filename="Interface/hud/spells/none.png" width="100%" height="100%"></image>
                                <panel id="spell_2_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="spell_2_cooldown_time" text="???" font="Interface/fonts/Verdana_18.fnt"/>
                                </panel>
                            </panel>
                            <panel width="2px"/>
                            <panel childLayout="center" width="58px" height="100%w">
                                <image id="spell_3_image" filename="Interface/hud/spells/none.png" width="100%" height="100%"></image>
                                <panel id="spell_3_cooldown" childLayout="center" width="100%" height="100%" backgroundColor="#000C">
                                    <text id="spell_3_cooldown_time" text="???" font="Interface/fonts/Verdana_18.fnt"/>
                                </panel>
                            </panel>
                            <panel width="14px"/>
                            <panel childLayout="vertical" width="39px">
                                <panel height="10px"/>
                                <image id="passive_image" filename="Interface/hud/spells/none.png" width="100%" height="100%w"></image>
                            </panel>
                            <panel/>
                        </panel>
                        <panel height="16px"/>
                        <panel childLayout="horizontal">
                            <panel width="108px"/>
                            <panel childLayout="vertical" width="360px" height="44px">
                                <panel width="100%" height="50%" backgroundColor="#4DE633FF"/>
                                <panel width="100%" height="50%" backgroundColor="#09CF"/>
                            </panel>
                            <panel/>
                        </panel>
                        <panel/>
                    </panel>
                    <panel width="*"/>
                </panel>
            </panel>
        </layer>
        <layer childLayout="center">
            <panel childLayout="vertical">
                <panel/>
                <panel childLayout="horizontal" height="160px">
                    <panel width="*"/>
                    <image filename="Interface/hud/center.png" width="575px" height="100%"></image>
                    <panel width="*"/>
                </panel>
            </panel>
        </layer>
        <layer childLayout="center">
            <panel childLayout="vertical">
                <panel height="21px"/>
                <panel childLayout="horizontal">
                    <panel/>
                    <image id="ping_bar" filename="Interface/hud/ping_bar.png" width="69px" height="14px"></image>
                    <panel width="21px"/>
                </panel>
            </panel>
        </layer>
        <layer childLayout="center">
            <panel childLayout="vertical">
                <panel height="20px"/>
                <panel childLayout="horizontal">
                    <panel/>
                    <image id="ping_container" filename="Interface/hud/ping_container.png" width="71px" height="16px">
                        <effect>
                            <onHover name="hint" hintText="Ping: ? ms"/>
                        </effect>
                    </image>
                    <panel width="20px"/>
                </panel>
            </panel>
        </layer>
        <layer id="death_recap_layer" childLayout="center" visible="false">
            <panel childLayout="vertical">
                <panel height="120px"/>
                <panel childLayout="horizontal" height="200px">
                    <panel/>
                    <panel id="death_recap_container" childLayout="vertical" width="400px">
                        <control name="button" width="100%" height="28px" label="Death Recap">
                            <interact onClick="toggleDisplayDeathRecap()"/>
                        </control>
                        <!--Generated ScrollPanel "death_recap"-->
                    </panel>
                    <panel/>
                </panel>
                <panel/>
            </panel>
        </layer>
        <layer childLayout="absolute">
            <control id="shop_window" name="window" title="Shop" x="150px" y="150px" width="525px" height="400px" hideOnClose="true" visible="false">
                <panel id="shop_container" childLayout="horizontal" width="100%" height="100%">
                    <panel childLayout="vertical" width="200px" height="100%" padding="10px" backgroundColor="#000D">
                        <panel childLayout="horizontal" height="32px">
                            <control id="shop_item_filter_text" name="textfield" width="100%"/>
                        </panel>
                        <panel childLayout="horizontal" height="32px">
                            <control id="shop_item_filter_checkbox_0" name="checkbox"/>
                            <panel width="6px"/>
                            <text text="All Items" font="Interface/fonts/Verdana_14.fnt" height="23px" checked="true"/>
                        </panel>
                        <panel childLayout="horizontal" height="32px">
                            <control id="shop_item_filter_checkbox_1" name="checkbox"/>
                            <panel width="6px"/>
                            <text text="Attack Damage" font="Interface/fonts/Verdana_14.fnt" height="23px"/>
                        </panel>
                        <panel childLayout="horizontal" height="32px">
                            <control id="shop_item_filter_checkbox_2" name="checkbox"/>
                            <panel width="6px"/>
                            <text text="Ability Power" font="Interface/fonts/Verdana_14.fnt" height="23px"/>
                        </panel>
                        <panel childLayout="horizontal" height="32px">
                            <control id="shop_item_filter_checkbox_3" name="checkbox"/>
                            <panel width="6px"/>
                            <text text="Movement Speed" font="Interface/fonts/Verdana_14.fnt" height="23px"/>
                        </panel>
                        <panel childLayout="horizontal" height="32px">
                            <control id="shop_item_filter_checkbox_4" name="checkbox"/>
                            <panel width="6px"/>
                            <text text="Teemo" font="Interface/fonts/Verdana_14.fnt" height="23px"/>
                        </panel>
                        <panel/>
                        <text text="Sell Item" font="Interface/fonts/Verdana_14.fnt" height="23px"/>
                        <panel childLayout="horizontal">
                            <control id="sell_item_button_0" name="button" width="32px" height="32px">
                                <interact onClick="sellItem(0)"/>
                            </control>
                            <control id="sell_item_button_1" name="button" width="32px" height="32px">
                                <interact onClick="sellItem(1)"/>
                            </control>
                            <control id="sell_item_button_2" name="button" width="32px" height="32px">
                                <interact onClick="sellItem(2)"/>
                            </control>
                        </panel>
                        <panel childLayout="horizontal">
                            <control id="sell_item_button_3" name="button" width="32px" height="32px">
                                <interact onClick="sellItem(3)"/>
                            </control>
                            <control id="sell_item_button_4" name="button" width="32px" height="32px">
                                <interact onClick="sellItem(4)"/>
                            </control>
                            <control id="sell_item_button_5" name="button" width="32px" height="32px">
                                <interact onClick="sellItem(5)"/>
                            </control>
                        </panel>
                    </panel>
                    <!--Generated ScrollPanel "shop_available_items"-->
                </panel>
            </control>
        </layer>
    </screen>
    <screen id="cinematic" controller="amara.engine.applications.ingame.client.gui.ScreenController_Cinematic">
    </screen>
    <screen id="gameOver" controller="amara.engine.applications.ingame.client.gui.ScreenController_GameOver">
        <layer childLayout="center" backgroundColor="#0008">
            <control name="button" x="35%" y="52%" width="40%" height="15%" label="Game Over">
                <effect>
                    <onStartScreen name="fade" length="1000"/>
                </effect>
                <interact onClick="exit()"/>
            </control>
        </layer>
    </screen>
</nifty>